{
  "name": "aws-cdk-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "bundle": "esbuild --bundle src/common/index.ts --outfile=cdk/bundle-common/nodejs/index.js --target=node14 --format=cjs",
    "bundleData": "cp -r src/common/data cdk/bundle-node_modules/nodejs/data",
    "set-log-retention": "aws logs describe-log-groups --query \"logGroups[?contains(logGroupName,'/aws/lambda/hello-jwt')]\" | jq -r \".[].logGroupName\" | sed \"s/^ *\\| *$//\"  | xargs -IVAR_LOG_GROUP_NAME aws logs put-retention-policy --log-group-name VAR_LOG_GROUP_NAME --retention-in-days 30"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hibohiboo/aws-cdk-v2.git"
  },
  "author": "hibohiboo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hibohiboo/aws-cdk-v2/issues"
  },
  "homepage": "https://github.com/hibohiboo/aws-cdk-v2#readme",
  "dependencies": {
    "aws-jwt-verify": "^3.1.0",
    "aws-sdk": "^2.1194.0",
    "date-fns": "^2.29.1",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.102",
    "@types/pg": "^8.6.5",
    "esbuild": "^0.15.2",
    "typescript": "^4.7.4"
  }
}